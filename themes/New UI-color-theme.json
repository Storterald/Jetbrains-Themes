	"tokenColors": [
        
        /**
         * Keywords
        */

        {
            "name": "keywords",
            "scope": [
                "storage.type.class",                
                "variable.language.this",
                "storage.type.namespace.directive",
                "storage.type.enum",
                "storage.type.extern",
                "storage.type.namespace.definition",
                "storage.type.template",
                "storage.type.struct",
                "constant.character.escape",

                // Generic
                "constant.language",
                "storage.modifier",
                "storage.type.modifier",
                "keyword"
            ],
            "settings": {
                "foreground": "#cb8c6c"
            }
        }, 	

        /**
         * Text
        */

        {
            "name": "text",
            "scope": [
                "string.quoted.other.lt-gt.include",
                "string.quoted.double.include",
                "entity.name.namespace",
                "variable.other.global",
                "variable.parameter.preprocessor",
                "entity.name.type.class.templated",
                "meta.preprocessor.conditional entity.name.function.preprocessor",
                "meta.preprocessor.macro entity.name.function.preprocessor",
                "storage.modifier.pointer",

                // Generic
                "storage.type",
                "entity.name.type",
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#bcbec4"
            }
        },

        /**
         * Strings
        */

        {
            "name": "strings",
            "scope": [
                "string",
            ],
            "settings": {
                "foreground": "#65a86d"
            }
        },

        /**
         * Numeric values
        */

        {
            "name": "numeric values",
            "scope": [
                "keyword.other.unit.hexadecimal",
                "keyword.other.unit.suffix.floating-point",
                "constant.numeric",
            ],
            "settings": {
                "foreground": "#299fa2"
            }
        },

        /**
         * Variables
        */

        {
            "name": "variables",
            "scope": [
                "support.type.property-name",

                // Generic
                "variable",
            ],
            "settings": {
                "foreground": "#c77dbb"
            }
        },

        /**
         * Comments
        */

        {
            "name": "comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#7d7d7d"
            }
        },

        /**
         * Documentation
        */

        {
            "name": "documentation",
            "scope": [
                "storage.type.class.doxygen",
                "comment.block.documentation",
                "comment.line.double-slash.documentation"
            ],
            "settings": {
                "foreground": "#5c7e67"
            }
        },
        {
            "name": "documentation words",
            "scope": [
                "comment.block.documentation variable.parameter"
            ],
            "settings": {
                "foreground": "#2f8683"
            }
        },

        /**
         * Functions
        */

        {
            "name": "function definition",
            "scope": [
                "entity.name.function.definition"
            ],
            "settings": {
                "foreground": "#56a8f5"
            }
        },
        {
            "name": "function",
            "scope": [
                "entity.name.function.call",
                "entity.name.function.operator.member",
                "entity.name.function.operator",
            ],
            "settings": {
                "foreground": "#bcbec4"
            }
        },

        /**
         * Other
        */

		{
			"scope": "comment.line.number-sign.ignore",
			"settings": {
                "foreground": "#56a8f5"
            }
		}, 		

		/**
         * Markdown
        */

        {
            "name": "markdown",
            "scope": [
                "markup"
            ],
            "settings": {
				"foreground": "#56a8f5"
			}
        },

        {
            "name": "markdown",
            "scope": [
                "heading.1.markdown",
                "punctuation.definition.heading.markdown",
                "entity.name.section.markdown"
            ],
            "settings": {
				"foreground": "#56a8f5",
				"fontStyle": "bold"
			}
        },

        {
            "name": "markdown italic",
            "scope": [
                "markup.italic.markdown"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },

        {
            "name": "markdown bold",
            "scope": [
                "markup.bold.markdown"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },

        {
            "name": "markdown strikethrough",
            "scope": [
                "markup.strikethrough.markdown"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },

        {
            "name": "markdown bold italic",
            "scope": [
                "markup.bold.markdown markup.italic.markdown",
                "markup.italic.markdown markup.bold.markdown"
            ],
            "settings": {
                "fontStyle": "italic bold"
            }
        },

        {
            "name": "markdown strikethrough bold",
            "scope": [
                "markup.bold.markdown markup.strikethrough.markdown",
                "markup.strikethrough.markdown markup.bold.markdown"
            ],
            "settings": {
                "fontStyle": "strikethrough bold"
            }
        },

        {
            "name": "markdown strikethrough italic",
            "scope": [
                "markup.italic.markdown markup.strikethrough.markdown",
                "markup.strikethrough.markdown markup.italic.markdown"
            ],
            "settings": {
                "fontStyle": "strikethrough italic"
            }
        },

        {
            "name": "markdown strikethrough italic bold",
            "scope": [
                "markup.bold.markdown markup.italic.markdown markup.strikethrough.markdown",
                "markup.strikethrough.markdown markup.bold.markdown markup.italic.markdown",
                "markup.strikethrough.markdown markup.italic.markdown markup.bold.markdown",
                "markup.italic.markdown markup.bold.markdown markup.strikethrough.markdown"
            ],
            "settings": {
                "fontStyle": "strikethrough italic bold"
            }
        },
    
        /**
         * CMAKE
        */	

        {
            "name": "cmake",
            "scope": [
                "storage.source.cmake",
            ],
            "settings": {
				"foreground": "#806a81"
			}
        },

        /**
         * HTML
        */ 

        {
            "name": "html",
            "scope": [
                "entity.name.tag.html",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#d5b778"
            }
        },

