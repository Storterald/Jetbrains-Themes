{
  "$schema": "vscode://schemas/color-theme",
  "name": "CLion New UI",
  "type": "dark",
  "include": "../common/dark.json",
  "tokenColors": [

    // #
    // #               GENERIC
    // # 

    {
      "name": "preprocessor keyword",
      "scope": [
        "keyword.directive.preprocessor",
        "keyword.control.directive"
      ],
      "settings": {
        "foreground": "#b3ae60"
      }
    },
    {
      "name": "macro",
      "scope": "entity.name.function.preprocessor",
      "settings": {
        "foreground": "#908b25"
      }
    },
    {
      "name": "types",
      "scope": [
        "source.cpp entity.name.scope-resolution"
      ],
      "settings": {
        "foreground": "#b5b6e3"
      }
    },
    {
      "name": "template constant",
      "scope": [
        "entity.name.type.template"
      ],
      "settings": {
        "foreground": "#c77dbb"
      }
    },
    {
      "name": "property",
      "scope": [
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#9373a5"
      }
    }
  ],
  "semanticTokenColors": {
    "newOperator": "#cf8e6d",
    "type": "#b5b6e3",
    "templateType": "#b5b6e3",
    "namespace": "#b5b6e3",
    "property": "#9373a5",
    "macro": "#908b25",
    "memberOperatorOverload": "#5f8c8a",
    "operatorOverload": "#5f8c8a",
    "function.declaration": "#56A8F5"
  }
}