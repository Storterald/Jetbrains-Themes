	"tokenColors": [

        {
            "name": "GENERIC keywords",
            "scope": [
                "keyword",
                "constant",
                "storage.type",
                "storage.modifier",
                "variable.language.this",
                "keyword.operator.wordlike"
            ],
            "settings": {
                "foreground": "#cf8e6d"
            }
        },
        {
            "name": "GENERIC operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#bcbec4"
            }
        },
        {
            "name": "GENERIC strings",
            "scope": [
                "string.quoted.other.lt-gt.include",
                "string.quoted.double.include",
                
                "string"
            ],
            "settings": {
                "foreground": "#6aab73"
            }
        },
        {
            "name": "GENERIC numeric",
            "scope": [
                "keyword.other.unit.hexadecimal",
                "keyword.other.unit.suffix.floating-point",
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#299fa2"
            }
        },
        {
            "name": "GENERIC comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#7d7d7d"
            }
        },
        {
            "name": "GENERIC documentation",
            "scope": [
                "storage.type.class.doxygen",
                "comment.block.documentation",
                "comment.line.double-slash.documentation"
            ],
            "settings": {
                "foreground": "#5e8163",
                "fontStyle": "italic"
            }
        },
        {
            "name": "GENERIC doxygen",
            "scope": [
                "storage.type.class.doxygen"
            ],
            "settings": {
                "foreground": "#67a37c",
                "fontStyle": "italic"
            }
        },
        {
            "name": "GENERIC documentation variable",
            "scope": [
                "comment.block.documentation variable.parameter"
            ],
            "settings": {
                "foreground": "#bcbec4",
                "fontStyle": "italic"
            }
        },

        

        {
            "name": "C/C++ directive",
            "scope": [
                "source.cpp keyword.control.directive"
            ],
            "settings": {
                "foreground": "#b3ae60"
            }
        },
        {
            "name": "C/C++ enum member",
            "scope": [
                "source.cpp variable.other.enummember"
            ],
            "settings": {
                "foreground": "#c77dbb",
                "fontStyle": "italic"
            }
        },
        {
            "name": "C/C++ keywords",
            "scope": [
                "source.cpp meta.encoding",
                "source.cpp keyword.operator.cast",
                "source.cpp keyword.operator.sizeof"
            ],
            "settings": {
                "foreground": "#cf8e6d"
            }
        },
        {
            "name": "C/C++ function definition",
            "scope": [
                "source.cpp entity.name.function.definition",
                "source.cpp entity.name.function.definition.special.constructor"
            ],
            "settings": {
                "foreground": "#56a8f5"
            }
        },
        {
            "name": "C/C++ preprocessor",
            "scope": "source.cpp entity.name.function.preprocessor",
            "settings": {
                "foreground": "#908b25"
            }
        },
        {
            "name": "C/C++ text",
            "scope": [
                "source.cpp storage.modifier.reference",
                "source.cpp storage.modifier.pointer"
            ],
            "settings": {
                "foreground": "#bcbec4"
            }
        },
        {
            "name": "C/C++ operators",
            "scope": [
                "source.cpp keyword.operator.bitwise.shift",
                "source.cpp punctuation.definition.begin.bracket.square",
                "source.cpp punctuation.definition.end.bracket.square"
            ],
            "settings": {
                "foreground": "#5f8c8a"
            }
        },



        {
            "name": "CMAKE keywords",
            "scope": "keyword.cmake",
            "settings": {
				"foreground": "#ffc66d"
			}
        },
        {
            "name": "CMAKE argument",
            "scope": "argument.source.cmake",
            "settings": {
                "foreground": "#6aab73"
            }
        },
        {
            "name": "CMAKE source",
            "scope": "entity.source.cmake",
            "settings": {
                "foreground": "#cf8e6d"
            }
        },



        {
            "name": "CSS",
            "scope": [
                "meta.property-value.css",
                "constant.other.unicode-range.css",
                "entity.other.attribute-name.pseudo-class.css",
