        {
            "name": "HTML tags",
            "scope": [
                "text.html.derivative entity.name.tag",
                "text.html.derivative punctuation.definition.tag.begin",
                "text.html.derivative punctuation.definition.tag.end"
            ],
            "settings": {
                "foreground": "#d5b778"
