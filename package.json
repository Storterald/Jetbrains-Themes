{
  "name": "jetbrains-themes-for-vs-code",
  "displayName": "Jetbrains themes",
  "icon": "logo.png",
  "description": "Jetbrains themes",
  "version": "1.0.0",
  "publisher": "Storterald",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Intellij New UI",
        "uiTheme": "vs-dark",
        "path": "./themes/Intellij New UI.json"
      },
      {
        "label": "CLion New UI",
        "uiTheme": "vs-dark",
        "path": "./themes/CLion New UI.json"
      }
    ],
    "configurationDefaults": {
      "editor.lineHeight": 1.65,
      "editor.fontFamily": "JetBrains Mono, Consolas, 'Courier New'"
    }
  },
  "devDependencies": {
    "esbuild": "^0.19.11"
  },
  "repository": {
    "url": "https://github.com/Storterald/intellij-idea-theme-for-vs-code"
  },
  "extensionDependencies": [
    "chadalen.vscode-jetbrains-icon-theme"
  ]
}
