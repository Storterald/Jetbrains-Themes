{
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    // #
    // #               GENERIC
    // # 
    "enumMember": "#c77dbb",

    // #
    // #               PYTHON
    // # 
    "class.builtin:python": "#8888C6",
    "function.builtin:python": "#8888C6",
    "builtinConstant:python": "#cf8e6d",
    "class.decorator:python": "#B3AE60",
    "function.decorator:python": "#B3AE60",
    "magicFunction.declaration:python": "#B200B2",
    "property:python": "#bcbec4",
    "selfParameter:python": "#94558D",
    "method.declaration:python": "#56A8F5",
    "module:python": "#bcbec4"
  }
}