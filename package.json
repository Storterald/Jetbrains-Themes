{
  "name": "jetbrains-themes",
  "displayName": "Jetbrains themes",
  "icon": "logo.png",
  "description": "Jetbrains themes",
  "version": "--version",
  "publisher": "Storterald",
  "engines": {
    "vscode": "^1.70.0"
  },
  "main": "./build/extension.js",
  "scripts": {
    "vscode:prepublish": "node ./src/createFontFiles.js && tsc -p ./"
  },
  "activationEvents": [
    "onCommand:workbench.action.selectTheme"
  ],
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "id": "jetbrains-new-ui-dark",
        "label": "Jetbrains New UI Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/Jetbrains New UI Dark.json"
      },
      {
        "id": "jetbrains-new-ui-light",
        "label": "Jetbrains New UI Light",
        "uiTheme": "vs",
        "path": "./themes/Jetbrains New UI Light.json"
      }
    ],
    "productIconThemes": [
      {
        "id": "jetbrains-new-ui-product-icons",
        "label": "Jetbrains New UI Product Icons",
        "path": "./icons/product-icon-theme.json"
      }
    ],
    "configurationDefaults": {
      "editor.lineHeight": 1.675,
      "workbench.tree.indent": 19,
      "editor.rulers": [
        120
      ],
      "editor.fontFamily": "JetBrains Mono, Consolas, 'Courier New'",
      "workbench.layoutControl.enabled": false,
      "workbench.productIconTheme": "jetbrains-new-ui-product-icons"
    }
  },
  "extensionDependencies": [
    "chadalen.vscode-jetbrains-icon-theme",
    "aaron-bond.better-comments"
  ],
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@types/vscode": "^1.70.0",
    "esbuild": "^0.19.11",
    "svg-path-properties": "^1.3.0",
    "typescript": "^5.6.3",
    "webfont": "^11.2.26",
    "glob": "^11.0.0"
  },
  "repository": {
    "url": "https://github.com/Storterald/Jetbrains-Themes"
  }
}